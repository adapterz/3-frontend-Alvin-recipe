{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": ["prettier"],
    "extends": ["eslint:recommended","plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi" :"error",
        "prettier/prettier": ["error", { "endOfLine": "auto"}],
        "no-unused-vars" : 0, // is defined but never used eslint 에러 발생 안시킴
        "useTabs" : false

    }
}
